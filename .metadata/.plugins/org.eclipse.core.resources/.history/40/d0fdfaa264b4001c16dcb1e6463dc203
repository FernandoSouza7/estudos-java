package fundamentos;

public class Temperatura {
	
	public static void main(String[] args) {
		double fahrenheit = 19;
		final double SUBTRACAO = 32;
		final double DIVISAO = 5/9;
		
		double conversaoDeTemperatura = (fahrenheit - SUBTRACAO) * DIVISAO;
		
		System.out.println(conversaoDeTemperatura);
	}
}
